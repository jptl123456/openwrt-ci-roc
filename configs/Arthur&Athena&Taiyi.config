# ====== 目标设备配置 ======
CONFIG_TARGET_qualcommax=y
CONFIG_TARGET_qualcommax_ipq60xx=y
CONFIG_TARGET_MULTI_PROFILE=y
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01=y

# ====== 彻底禁用无线 ======
# ATH11K内存配置（无线相关）
# CONFIG_ATH11K_MEM_PROFILE_512M is not set
# CONFIG_ATH11K_MEM_PROFILE_256M is not set
# CONFIG_ATH11K_MEM_PROFILE_1G is not set

# NSS固件版本（使用低版本避免无线依赖）
# CONFIG_NSS_FIRMWARE_VERSION_12_5 is not set
# CONFIG_NSS_FIRMWARE_VERSION_12_6 is not set
CONFIG_NSS_FIRMWARE_VERSION_11_4=y

# 无线相关应用包
# CONFIG_PACKAGE_luci-app-gecoosac is not set

# ====== 基础包配置 ======
CONFIG_PACKAGE_luci-app-openlist2=y
CONFIG_PACKAGE_luci-app-athena-led=m
CONFIG_PACKAGE_luci-i18n-athena-led-zh-cn=m
CONFIG_PACKAGE_luci-app-lucky=m
# CONFIG_PACKAGE_luci-app-dockerman is not set

# ====== NSS有线驱动（保留） ======
CONFIG_PACKAGE_kmod-qca-nss-drv=y
CONFIG_PACKAGE_kmod-qca-nss-ecm=y
CONFIG_PACKAGE_kmod-qca-nss-crypto=y
CONFIG_PACKAGE_kmod-qca-nss-cfi=y
CONFIG_PACKAGE_kmod-qca-nss-dp=y

# ====== 无线框架和驱动（彻底禁用） ======
# 基础无线框架
# CONFIG_PACKAGE_kmod-mac80211 is not set
# CONFIG_PACKAGE_kmod-cfg80211 is not set

# IPQ60xx无线驱动
# CONFIG_PACKAGE_kmod-ath11k is not set

# 无线固件
# CONFIG_PACKAGE_ath11k-firmware is not set
# CONFIG_PACKAGE_ath11k-firmware-ipq6018 is not set
# CONFIG_PACKAGE_ipq-wifi is not set
# CONFIG_PACKAGE_wireless-regdb is not set

# 无线工具和服务
# CONFIG_PACKAGE_wifi-scripts is not set
# CONFIG_PACKAGE_iw is not set
# CONFIG_PACKAGE_hostapd is not set
# CONFIG_PACKAGE_wpa-supplicant is not set
# CONFIG_PACKAGE_iwinfo is not set
